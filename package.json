{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.9.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.25.0",
    "formik": "^2.2.9",
    "history": "^4.10.1",
    "i18next": "^20.6.1",
    "i18next-browser-languagedetector": "^6.1.3",
    "moment": "^2.29.1",
    "msw": "^0.36.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.15.4",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-react": "^6.0.2",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --no-cache",
    "test:coverage": "react-scripts test --no-cache --coverage --watchAll=false",
    "test:ci": "react-scripts test --no-cache --coverage --coverageReporters=cobertura --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "npm run lint -- --fix",
    "preinstall": "npx npm-force-resolutions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nodelib/fs.stat": "^2.0.5",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "json-server": "^0.16.3",
    "node-sass": "^7.0.0",
    "react-error-overlay": "6.0.9"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public/"
    ],
    "coveragePathIgnorePatterns": [
      "src/api",
      "src/test",
      "src/i18n.ts",
      "src/index.tsx",
      "src/dataMappers",
      "src/store/hooks.ts",
      "src/store/index.ts",
      "src/store/events.ts",
      "src/types",
      "src/locales"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 55,
        "functions": 70,
        "lines": 85,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
